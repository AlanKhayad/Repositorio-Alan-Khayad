number1=input("Enter the first number:")
number2=input("Enter the second number:")
is_valid=true

if number1.isnumeric():
    number1=float(number1)
else:
    is_valid=False

if number2.isnumeric():
    number2=float(number2)
else:
    is_valid=False


if is_valid:
    if number2==0:
        print("Error")

if is_valid:
    print(number1/number2)